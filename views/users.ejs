<html>
<head>
<meta name="viewport" content="initial-scale=1">
<link href='http://fonts.googleapis.com/css?family=Open+Sans' rel='stylesheet' type='text/css'>
<link href='http://fonts.googleapis.com/css?family=Playfair+Display' rel='stylesheet' type='text/css'>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
<link rel="stylesheet" href="../styles.css">
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.1/js/bootstrap.min.js"></script>
</head>
<body style="background-color:white;">

 <nav class="navbar navbar-default navbar-fixed-top">
  <div class="container col-sm-8 col-sm-offset-2">
    <!-- Brand and toggle get grouped for better mobile display -->
    <div class="navbar-header">
      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a class="navbar-brand" href="#">Brand</a>
    </div>

    <!-- Collect the nav links, forms, and other content for toggling -->
    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
  <ul class="nav navbar-nav">
        <li> <a href="/">Home</a></li>
        <li><a href="../items"> items</a></li>
        <li class="active"><%if(isAuthenticated==true){%><a href="<%=user.user.summoner%>"><span class="sr-only">(current)</span>  User</a><%}else{%>
        <a href="/">User</a> <span class="sr-only">(current)</span> <%}%>
        </li>
        <li><a href="../champions">Champions</a></li>
 </ul>

    </div><!-- /.navbar-collapse -->
  </div><!-- /.container-fluid -->
</nav>


<div class='container'>
<%for(var i = 0; i<resuserstatsget.matches.length;i++){%>
<div class = "match-container col-lg-12">
 <div class = "col-lg-12 summ-background" style="background-image: url(http://lkimg.zamimg.com/shared/images/champion_headers/<%=resuserstatsget.matches[i].participants[0].championId%>_0.jpg)">
</div>
<div class='match-overview col-lg-12'>
<div class='col-lg-4'><ul>Match Type:
<li><%=resuserstatsget.matches[i].queueType%>
</ul></div>
<div class='col-lg-4'><ul>Time:
<li><%=Math.round(resuserstatsget.matches[i].matchDuration/60)%> Minutes</li>
</ul></div>
<div class='col-lg-4'><ul>When:
<li><% var d = new Date(); 
	var times = resuserstatsget.matches[i].matchCreation;
	 d.setTime(times);
%>
<%=d%>
</li>
</ul></div>
</div>
<div class='col-lg-12 matchdetails'>

<div class='col-lg-4 user-champion'><img class='champ-75'src="http://lkimg.zamimg.com/shared/riot/images/champions/<%=resuserstatsget.matches[i].participants[0].championId%>"></div>

<div class='col-lg-4 user-items'>
<%if( resuserstatsget.matches[i].participants[0].stats.item0 !== 0){%> 	<img src='http://lkimg.zamimg.com/shared/riot/images/items/<%=resuserstatsget.matches[i].participants[0].stats.item0%>_32.png'><%}%>
	
<%if( resuserstatsget.matches[i].participants[0].stats.item1 !== 0){%> 	<img src='http://lkimg.zamimg.com/shared/riot/images/items/<%=resuserstatsget.matches[i].participants[0].stats.item1%>_32.png'><%}%>

<%if( resuserstatsget.matches[i].participants[0].stats.item2 !== 0){%> 	<img src='http://lkimg.zamimg.com/shared/riot/images/items/<%=resuserstatsget.matches[i].participants[0].stats.item2%>_32.png'><%}%>

<%if( resuserstatsget.matches[i].participants[0].stats.item3 !== 0){%> 	<img src='http://lkimg.zamimg.com/shared/riot/images/items/<%=resuserstatsget.matches[i].participants[0].stats.item3%>_32.png'>	<%}%>

<%if( resuserstatsget.matches[i].participants[0].stats.item4 !== 0){%> 	<img src='http://lkimg.zamimg.com/shared/riot/images/items/<%=resuserstatsget.matches[i].participants[0].stats.item4%>_32.png'>	<%}%>

<%if( resuserstatsget.matches[i].participants[0].stats.item5 !== 0){%> 	<img src='http://lkimg.zamimg.com/shared/riot/images/items/<%=resuserstatsget.matches[i].participants[0].stats.item5%>_32.png'>
	<%}%>

<%if( resuserstatsget.matches[i].participants[0].stats.item6 !== 0){%> 	<img src='http://lkimg.zamimg.com/shared/riot/images/items/<%=resuserstatsget.matches[i].participants[0].stats.item6%>_32.png'>
	<%}%>
</div>
<div class='col-lg-4 user-stats'>
<ul>
<li>Results:<% if ( resuserstatsget.matches[i].participants[0].stats.winner == true){%>
Win
<%}else{%> Loss<%}%> </li>
<li>Kills: <%=resuserstatsget.matches[i].participants[0].stats.kills%></li>
<li>Deaths:<%=resuserstatsget.matches[i].participants[0].stats.deaths%></li>
<li>Assists:<%=resuserstatsget.matches[i].participants[0].stats.assists%></li>
</ul>
</div>
</div>
</div><!--matchcontainer-->
<%}%>

</body>
</html>
